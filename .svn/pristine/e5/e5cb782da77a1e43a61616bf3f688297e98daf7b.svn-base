// tbody的tr类
class trObject {
  constructor(list, filter_visible = true, tree_visible = false, is_tree = false) {
    this.list = list // td列表
    this.filter_visible = filter_visible // tr行是否显示:过滤器
    this.tree_visible = tree_visible // tr行是否显示:树形展开
    this.is_tree = is_tree // 是否是树形展开行
    this.expand_visible = false // 展开行是否展开
    this.$children_list = null // 生成后的树形列表
    this.loading_tree = false // 树形记载中
  }
}
export default trObject
