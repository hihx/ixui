@import "./var.less";
.ix-tree{
  .ix-tree-item{
    display: block;
    padding: 3px 6px;
    &:hover{
      background: @theme-low;
      border-radius: 2px;
    }
  }
  .ix-icon-tree{
    font-size: 12px;
    color: #c0c4cc;
    transition: transform .3s ease-in-out;
  }
  .ix-tree-group{
    margin-left: 16px;
    overflow: hidden;
  }
}
